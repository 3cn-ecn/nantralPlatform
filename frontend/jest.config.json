{
  "transform": {
    "^.+\\.tsx?$": "ts-jest"
  },
  "moduleNameMapper": {
    "\\.(gif|ttf|eot|svg|png)$": "<rootDir>/test/__mocks__/fileMock.js",
    "\\.(css|less|sass|scss)$": "identity-obj-proxy",
    "^#(.*)": "<rootDir>/src/$1"
  },
  "testEnvironment": "jsdom",
  "maxWorkers": 2,
  "snapshotSerializers": ["@emotion/jest/serializer"]
}
